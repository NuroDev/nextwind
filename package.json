		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"dependencies": {
		"next": "12.0.5",
		"next-themes": "^0.0.15",
		"react": "17.0.2",
		"react-dom": "17.0.2"
	},
	"devDependencies": {
		"@types/node": "^16.11.9",
		"@types/react": "^17.0.35",
		"@types/react-dom": "^17.0.11",
		"eslint": "7",
		"eslint-config-next": "12.0.4",
		"prettier": "^2.4.1",
		"typescript": "^4.5.2",
		"windicss": "^3.2.1",
		"windicss-webpack-plugin": "^1.5.8"
	}
}
