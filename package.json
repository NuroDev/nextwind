{
	"private": true,
	"name": "nextwind",
	"description": "Next.js + EmotionCSS + WindiCSS project template",
	"version": "0.1.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/nurodev/nextwind.git"
	},
	"homepage": "https://github.com/nurodev/nextwind",
	"bugs": "https://github.com/nurodev/nextwind/issues",
	"author": {
		"name": "nurodev",
		"email": "me@nuro.dev",
		"url": "https://nuro.dev"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\""
	},
	"prettier": {
		"arrowParens": "always",
